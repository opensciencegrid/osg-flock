      <attr name="USE_MATCH_AUTH" glidein_publish="False" job_publish="False" parameter="True" type="string" value="True"/>
      <attr name="SYSLOG_HOST" parameter="False" type="expr" value="syslog.osgdev.chtc.io"/>
